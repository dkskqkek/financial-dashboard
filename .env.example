# API Base URL
VITE_API_BASE_URL=http://localhost:3001/api

# WebSocket Settings
VITE_WS_URL=ws://localhost:3001/ws
VITE_ENABLE_WEBSOCKET=false

# Stock API Keys (무료 가입 후 사용)
# Alpha Vantage: https://www.alphavantage.co/support/#api-key
VITE_ALPHA_VANTAGE_KEY=your_alpha_vantage_key

# Polygon.io: https://polygon.io/
VITE_POLYGON_KEY=your_polygon_key

# IEX Cloud: https://iexcloud.io/
VITE_IEX_TOKEN=your_iex_token

# Finnhub: https://finnhub.io/
VITE_FINNHUB_KEY=your_finnhub_key

# 한국투자증권 API
VITE_KIS_APP_KEY=your_kis_app_key
VITE_KIS_SECRET_KEY=your_kis_secret_key

# CORS Proxy Settings
VITE_USE_CORS_PROXY=true
VITE_PREFERRED_PROXY=https://cors-anywhere.herokuapp.com/

# Development Settings
VITE_MOCK_API=true
VITE_DEBUG_API=true
