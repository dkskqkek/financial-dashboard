    "lint:fix": "eslint . --ext ts,tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,css,md}\"",
    "type-check": "npx tsc --noEmit",
    "preview": "vite preview",
    "prepare": "npm run format && npm run lint:fix",
    "ci": "npm run type-check && npm run lint && npm run format:check && npm run build",
    "pr": "npm run prepare && git add . && git commit -m \"feat: automated changes\" && git push && gh pr create --fill",
